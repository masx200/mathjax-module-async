{
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "name": "@masx200/mathjax-module-async",
    "version": "1.0.1",
    "description": "把mathjax转化成模块形式异步加载导出,并且不会污染全局变量",
